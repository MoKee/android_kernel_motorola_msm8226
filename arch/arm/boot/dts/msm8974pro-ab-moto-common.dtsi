/include/ "msm8974pro-pm8941.dtsi"
/include/ "msm8974-leds.dtsi"
/include/ "msm8974-camera-sensor-moto.dtsi"
/ {
	aliases {
		uart3 = &uart3;
                spi12 = &spi_12;
	};

	cycapsense_prog {
			compatible = "cypress,cycapsense_prog";
			label = "cy8c20237_24lkxi";
			silicon_id = <0x0142>;
			block_size = <128>;
			num_of_blocks = <64>;
			secure_bytes = <16>;
			gpios = <&msmgpio 73 0>, /* RESET */
					<&msmgpio 111 0>, /* programming SCLK */
					<&msmgpio 110 0>; /* programming SDAT */
	};
};

&soc {
	qcom,ion {
		qcom,ion-heap@23 { /* OTHER PIL HEAP */
			qcom,memory-fixed = <0x04b00000 0x2700000>;
		};

		qcom,ion-heap@26 { /* MODEM HEAP */
			qcom,memory-fixed = <0x07700000 0x5a00000>;
		};
	};

	serial@f991d000 {       /* "UART_AP_*_SP" on BLSP1 (BLSP1 UART0) */
		compatible = "qcom,msm-lsuart-v14";
		reg = <0xf991d000 0x1000>;
		interrupts = <0 107 0>;
		status = "ok";
	};

	serial@f991e000 {       /* "UART1DM" on BLSP2 (BLSP1 UART1) */
		status = "ok";
	};

	hob_ram@0fbe0000 {
		compatible = "mmi,hob_ram";
		reg = <0x0fbe0000 0x1e000>,  /* hob address */
			<0x0fbfe000 0x2000>; /* dhob address */
	};

	ram_console {
		compatible = "android,ram-console";
		status = "ok";

		android,ram-buffer-start = <0x3eae0000>;
		android,ram-buffer-size = <0x20000>;
	};

	bootinfo {
		compatible = "android,bootinfo";
		status = "ok";

		android,bootinfo-buffer-start = <0x3eb00000>;
		android,bootinfo-buffer-size = <0x800>;
	};


	i2c_0: i2c@f9967000 {   /* Unused on BLSP11 (BLSP2 QUP4) */
		status = "disabled";
	};

	i2c_1: i2c@f9923000 {   /* "I2C4" on BLSP1 (BLSP1 UART0) */
		status = "ok";
	};

	i2c_2: i2c@f9924000 {   /* "I2C5" on BLSP2 (BLSP1 QUP1) */
		status = "ok";
	};

	i2c_3: i2c@f9927000 {   /* "I2C1" on BLSP5 (BLSP1 QUP4) */
		cell-index = <3>;
		compatible = "qcom,i2c-qup";
		reg = <0xf9927000 0x1000>;
		#address-cells = <1>;
		#size-cells = <0>;
		reg-names = "qup_phys_addr";
		interrupts = <0 99 0>;
		interrupt-names = "qup_err_intr";
		qcom,i2c-bus-freq = <100000>;
		qcom,i2c-src-freq = <50000000>;
		qcom,master-id = <86>;
	};

	i2c_4: i2c@f9964000 {   /* "I2C2" on BLSP8 (BLSP2 QUP1) */
		cell-index = <4>;
		compatible = "qcom,i2c-qup";
		reg = <0xf9964000 0x1000>;
		#address-cells = <1>;
		#size-cells = <0>;
		reg-names = "qup_phys_addr";
		interrupts = <0 102 0>;
		interrupt-names = "qup_err_intr";
		qcom,i2c-bus-freq = <100000>;
		qcom,i2c-src-freq = <50000000>;
		qcom,master-id = <84>;
	};

	i2c_5: i2c@f9926000 {   /* "I2C3" on BLSP4 (BLSP1 QUP3) */
		cell-index = <5>;
		compatible = "qcom,i2c-qup";
		reg = <0xf9926000 0x1000>;
		#address-cells = <1>;
		#size-cells = <0>;
		reg-names = "qup_phys_addr";
		interrupts = <0 98 0>;
		interrupt-names = "qup_err_intr";
		qcom,i2c-bus-freq = <100000>;
		qcom,i2c-src-freq = <50000000>;
		qcom,master-id = <86>;
	};

	i2c_6: i2c@f9966000 {   /* BLSP10 (BLSP2 QUP3) */
		status = "disabled";
	};

	spi_0: spi@f9923000 {
		status = "disabled";
	};

        /* BLSP12 (BLSP2 QUP5) VALIDITY CHANGE */
        spi_12: spi@f9968000 {
		compatible = "qcom,spi-qup-v2";
		cell-index = <12>;

		reg = <0xf9968000 0x1000>, <0xf9944000 0x19000>;
		reg-names = "spi_physical", "spi_bam_physical";

		interrupts = <0 106 0>, <0 239 0>;
		interrupt-names = "spi_irq", "spi_bam_irq";

		spi-max-frequency = <48000000>;
		#address-cells = <1>;
		#size-cells = <0>;
		gpios = <&msmgpio 88 0>,
			<&msmgpio 86 0>,
			<&msmgpio 85 0>;
		cs-gpios = <&msmgpio 87 0>;
		qcom,master-id = <84>;

		qcom,use-bam;
		qcom,bam-consumer-pipe-index = <22>;
		qcom,bam-producer-pipe-index = <23>;
		qcom,ver-reg-exists;

		validity-metallica@0{
			compatible = "validity,metallica";
			reg = <0>;
			spi-max-frequency = <17143000>;
		};
	};

	uart3: uart@f991f000 {  /* "UART_AP_*_C55" on BLSP3 (BLSP1 UART2) */
		compatible = "qcom,msm-hsuart-v14";
		reg = <0xf991f000 0x1000>,
			<0xf9904000 0x19000>;
		reg-names = "core_mem", "bam_mem";
		interrupts = <0 109 0>, <0 238 0>;
		interrupt-names = "core_irq", "bam_irq";
		qcom,bam-tx-ep-pipe-index = <4>;
		qcom,bam-rx-ep-pipe-index = <5>;
		qcom,msm-bus,name = "uart3";
		qcom,msm-bus,num-cases = <2>;
		qcom,msm-bus,num-paths = <1>;
		qcom,msm-bus,vectors-KBps =
				<86 512 0 0>,
				<86 512 500 800>;
	};

	qcom,hdmi_tx@fd922100 {
		status = "disabled";
	};

	gpio_keys {
		compatible = "gpio-keys";
		input-name = "gpio-keys";

		camera_snapshot {
			label = "camera_snapshot";
			gpios = <&pm8941_gpios 3 0x1>;
			linux,input-type = <1>;
			linux,code = <0x2fe>;
			gpio-key,wakeup;
			debounce-interval = <15>;
		};

		vol_down {
			label = "volume_down";
			gpios = <&pm8941_gpios 2 0x1>;
			linux,input-type = <1>;
			linux,code = <114>;
			gpio-key,wakeup;
			debounce-interval = <15>;
		};

		vol_up {
			label = "volume_up";
			gpios = <&pm8941_gpios 5 0x1>;
			linux,input-type = <1>;
			linux,code = <115>;
			gpio-key,wakeup;
			debounce-interval = <15>;
		};
	};

	sound {
		qcom,model = "msm8974-taiko-mtp-snd-card";
		qcom,cdc-micbias2-headset-only;
	};

	vbat_disp_vreg: vbat_disp_vreg {
		compatible = "regulator-fixed";
		regulator-name = "vbat_disp";
		gpio = <&pm8941_gpios 14 0x0>;
		parent-supply = <&pm8941_boost>;
		startup-delay-us = <300>;
		enable-active-high;
	};

	vdd_disp_vreg: vdd_disp_vreg {
		compatible = "regulator-fixed";
		regulator-name = "vdd_disp";
		gpio = <&pm8941_gpios 23 0x0>;
		parent-supply = <&pm8941_s3>;
		startup-delay-us = <300>;
		enable-active-high;
	};

	vci_disp_vreg: vci_disp_vreg {
		compatible = "regulator-fixed";
		regulator-name = "vci_disp";
		gpio = <&pm8941_gpios 19 0>;
		parent-supply = <&pm8941_l23>;
		startup-delay-us = <300>;
		enable-active-high;
	};

	qcom,msm-adsp-sensors {
//		status = "disabled";
	};

	ti,c55-ctrl {
		compatible = "ti,c55-ctrl";
		reg = <0 0>;
		gpios = <&msmgpio 77 2>,        /* gpio_core */
			<&msmgpio 125 0>,       /* gpio_reset */
			<&msmgpio 79 1>,        /* gpio_ap_int */
			<&msmgpio 124 2>;       /* gpio_c55_int */
	};

	factory_support {
		compatible = "mmi,factory-support-msm8960";
		gpios = <&pm8941_gpios 36 0x2>; /* GPIOF_OUT_INIT_HIGH */
		gpio-names = "factory_kill_disable";
	};

	qcom,msm-dai-mi2s {
		compatible = "qcom,msm-dai-mi2s";
		qcom,msm-dai-q6-mi2s-prim {
			compatible = "qcom,msm-dai-q6-mi2s";
			qcom,msm-dai-q6-mi2s-dev-id = <0>;
			qcom,msm-mi2s-rx-lines = <0>;
			qcom,msm-mi2s-tx-lines = <1>;
		};
	};

	mmi_batterydata: qcom,battery-data {
		qcom,rpull-up-kohm = <100>;
		qcom,vref-batt-therm-uv = <1800000>;
		qcom,default-kohm = <10>;

		/include/ "batterydata-ed30-temp.dtsi"
		/include/ "batterydata-ed30-lishen.dtsi"
		/include/ "batterydata-ed30-lg.dtsi"
	};
};

&mdss_mdp {
	qcom,mdss-pref-prim-intf = "dsi";
};

&mdss_dsi0 {
	qcom,platform-reset-gpio = <&pm8941_gpios 12 0>;

	/* Remove unneeded DSI properties / supplies */
	/delete-property/ qcom,platform-enable-gpio;
	/delete-property/ qcom,platform-reset-sequence;

	/delete-node/ qcom,platform-supply-entry1;
};

&spmi_bus {
	qcom,pm8941@1 {

		qcom,leds@d000 {
			compatible = "qcom,leds-qpnp-rgb";
			qcom,rgb_0 {
				/delete-property/ qcom,default-state;
				/delete-property/ linux,default-trigger;
			};

			qcom,rgb_1 {
				/delete-property/ qcom,default-state;
				/delete-property/ linux,default-trigger;
			};

			qcom,rgb_2 {
				status = "ok";
			};

			qcom,rgb_3 {
				label = "combo";
				linux,name = "rgb";
				qcom,max-current = <12>;
				qcom,id = <9>;
			};

		};

		qcom,leds@d800 {
			status = "okay";
			qcom,wled_0 {
				label = "wled";
				linux,name = "wled:backlight";
				linux,default-trigger = "bkl-trigger";
				qcom,cs-out-en;
				qcom,op-fdbck = <1>;
				qcom,default-state = "on";
				qcom,max-current = <25>;
				qcom,ctrl-delay-us = <0>;
				qcom,boost-curr-lim = <3>;
				qcom,cp-sel = <0>;
				qcom,switch-freq = <2>;
				qcom,ovp-val = <2>;
				qcom,num-strings = <1>;
				qcom,id = <0>;
			};
		};

		qcom,leds@d900 {
			status = "disabled";
		};

		qcom,leds@da00 {
			status = "disabled";
		};

		qcom,leds@db00 {
			status = "disabled";
		};

		qcom,leds@dc00 {
			status = "disabled";
		};

		qcom,leds@dd00 {
			status = "disabled";
		};

		qcom,leds@de00 {
			status = "disabled";
		};

		qcom,leds@df00 {
			status = "disabled";
		};

		qcom,leds@e000 {
			status = "disabled";
		};

		qcom,leds@e100 {
			status = "disabled";
		};

		qcom,leds@e200 {
			status = "disabled";
		};
	};

	qcom,pm8941@0 {

		pm8941_chg_fac: qcom,chargerfac {
			spmi-dev-container;
			compatible = "qcom,qpnp-charger-factory";
			#address-cells = <1>;
			#size-cells = <1>;
			status = "ok";

			qcom,chg-vddmax-mv = <4200>;
			qcom,chg-vddsafe-mv = <4200>;
			qcom,chg-vinmin-mv = <4200>;
			qcom,chg-ibatmax-ma = <1500>;
			qcom,chg-ibatsafe-ma = <1500>;
			qcom,chg-thermal-mitigation = <1500 700 600 325>;
			qcom,chg-cool-bat-decidegc = <100>;
			qcom,chg-cool-bat-mv = <4100>;
			qcom,chg-ibatmax-warm-ma = <350>;
			qcom,chg-warm-bat-decidegc = <450>;
			qcom,chg-warm-bat-mv = <4100>;
			qcom,chg-ibatmax-cool-ma = <350>;
			qcom,chg-vbatdet-mv = <4100>;

			qcom,chg-chgr@1000 {
				status = "ok";
				reg = <0x1000 0x100>;
				interrupts =	<0x0 0x10 0x0>,
						<0x0 0x10 0x1>,
						<0x0 0x10 0x2>,
						<0x0 0x10 0x3>,
						<0x0 0x10 0x4>,
						<0x0 0x10 0x5>,
						<0x0 0x10 0x6>,
						<0x0 0x10 0x7>;

				interrupt-names =	"vbat-det-lo",
							"vbat-det-hi",
							"chgwdog",
							"state-change",
							"trkl-chg-on",
							"fast-chg-on",
							"chg-failed",
							"chg-done";
			};

			qcom,chg-buck@1100 {
				status = "ok";
				reg = <0x1100 0x100>;
				interrupts =	<0x0 0x11 0x0>,
						<0x0 0x11 0x1>,
						<0x0 0x11 0x2>,
						<0x0 0x11 0x3>,
						<0x0 0x11 0x4>,
						<0x0 0x11 0x5>,
						<0x0 0x11 0x6>;

				interrupt-names =	"vbat-ov",
							"vreg-ov",
							"overtemp",
							"vchg-loop",
							"ichg-loop",
							"ibat-loop",
							"vdd-loop";
			};

			qcom,chg-bat-if@1200 {
				status = "ok";
				reg = <0x1200 0x100>;
				interrupts =	<0x0 0x12 0x0>,
						<0x0 0x12 0x1>,
						<0x0 0x12 0x2>,
						<0x0 0x12 0x3>,
						<0x0 0x12 0x4>;

				interrupt-names =	"batt-pres",
							"bat-temp-ok",
							"bat-fet-on",
							"vcp-on",
							"psi";

			};

			qcom,chg-usb-chgpth@1300 {
				status = "ok";
				reg = <0x1300 0x100>;
				interrupts =	<0 0x13 0x0>,
						<0 0x13 0x1>,
						<0x0 0x13 0x2>;

				interrupt-names =	"coarse-det-usb",
							"usbin-valid",
							"chg-gone";
			};

			qcom,chg-dc-chgpth@1400 {
				status = "ok";
				reg = <0x1400 0x100>;
				interrupts =	<0x0 0x14 0x0>,
						<0x0 0x14 0x1>;

				interrupt-names =	"coarse-det-dc",
							"dcin-valid";
			};

			qcom,chg-boost@1500 {
				status = "ok";
				reg = <0x1500 0x100>;
				interrupts =	<0x0 0x15 0x0>,
						<0x0 0x15 0x1>;

				interrupt-names =	"boost-pwr-ok",
							"limit-error";
			};

			qcom,chg-misc@1600 {
				status = "ok";
				reg = <0x1600 0x100>;
			};
		};
	};
};

